<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ebookprojectback.mapper.BookMapper">
    
    <select id="select_book_by_no" resultType="com.ebookprojectback.dto.BookDTO">
        SELECT *
        FROM books B
        where no = #{no};

    </select>

</mapper>